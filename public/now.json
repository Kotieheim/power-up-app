{
  "version": 2,
  "alias": "koties-power-up-app",
  "name": "power-up-app",
  "routes": [
    {
      "src": "^/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": ".*",
      "dest": "/index.html"
    }
  ],
  "build": {
    "env": {
      "REACT_APP_API_ENDPOINT": "https://koties-power-up-app.herokuapp.com/api"
    }
  }
}
